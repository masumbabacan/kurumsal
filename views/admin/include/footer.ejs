<script>
    $(document).ready(function () {
    let sideelements =  ['dashboard','users','services'];
        let href = $(location).attr('href');
        sideelements.forEach(element => {
        if (href.indexOf(element) > -1) {
            $("." + element).addClass("active");
            $("." + element).parent().parent().addClass("active open");
        }
        });
    });
    $("#logout").on('click',(e) => {
        $.ajax({
            url: 'http://localhost:3000/api/kurumsal/auth/logout',
            type: 'DELETE',
            success: function(result) {
                window.location.href = "http://localhost:3000/api/kurumsal/auth/login";
            }
        });
    });
</script>
